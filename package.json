{
  "name": "@andyinabox/viz-flowchart",
  "version": "0.1.1",
  "description": "A d3-based flow chart web component",
  "source": "src/index.js",
  "module": "dist/viz-flowchart.module.js",
  "main": "dist/viz-flowchart.js",
