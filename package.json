  },
  "peerDependencies": {
    "d3": "^5.15.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "servor": "^3.2.0",
    "rollup": "^2.1.0"
  }
