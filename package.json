    "url": "https://github.com/andyinabox/viz-flowchart/issues"
  },
  "homepage": "https://github.com/andyinabox/viz-flowchart#readme",
  "dependencies": {},
  "peerDependencies": {
    "d3": "^5.15.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "microbundle": "^0.11.0",
    "servor": "^3.2.0"
  }
}
